<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Frequency</th>
      <th>Offset</th>
    </tr>
  </thead>
  <tbody>
{{#each repeaters}}{{#if linked}}
    <tr>
      <td>{{name}}</td>
      <td>{{frequency}} MHz</td>
      <td>{{offset}}</td>
    </tr>
{{/if}}{{/each}}
  </tbody>
</table>

<p>
  View these repeaters on our
  <a href="{{relative to 'map.html'}}">interactive map page</a>.
</p>
