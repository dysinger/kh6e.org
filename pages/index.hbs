---
title: KauaiARC
layout: raw.hbs
---
<!DOCTYPE html>
<html lang="en">
  <head>
    {{> head }}
    <link href="{{assets}}/css/index.css" rel="stylesheet">
  </head>
  <body id="page-top" class="index">
    {{> nav }}
    <header id="splash">
      <div class="container">
        <div class="intro-text">
          <div class="intro-lead-in">Welcome to</div>
          <div class="intro-heading">Kauai Amateur Radio Club</div>
        </div>
      </div>
    </header>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          {{md 'content/irc.md'}}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          {{md 'content/monday_net.md'}}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          {{md 'content/breakfast.md'}}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          {{md 'content/pres_msg.md'}}
        </div>
      </div>
    </div>
    {{> footer }}
    <script src="{{assets}}/classie/classie.js"></script>
    <script src="{{assets}}/js/cbpAnimatedHeader.js"></script>
  </body>
</html>
